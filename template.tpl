// If the user chose to log debug output, initialize the logging method
const log = data.debug ? logToConsole : (() => {});

// If the script loaded successfully, log a message and signal success
const onSuccess = () => {
  log('EXACTIUS: Script loaded successfully.');
  data.gtmOnSuccess();
};

// If the script fails to load, log a message and signal failure
const onFailure = () => {
  log('EXACTIUS: Script load failed.');
  data.gtmOnFailure();
};



// use the provided APIs to do things like send pixels
const url = 'https://dnqwern1hex09.cloudfront.net/configs/' + encodeUriComponent(partner) + '.js' ;
injectScript(url, data.gtmOnSuccess, data.gtmOnFailure, url);


